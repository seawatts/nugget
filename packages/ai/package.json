{
  "dependencies": {
    "@boundaryml/baml": "0.213.0"
  },
  "devDependencies": {
    "@nugget/tsconfig": "workspace:*",
    "typescript": "catalog:"
  },
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/baml_client/*.ts",
    "./react/*": "./src/baml_client/react/*.ts",
    "./async_client": "./src/baml_client/async_client.ts",
    "./celebration-orchestrator": "./src/celebration-orchestrator.ts"
  },
  "license": "MIT",
  "name": "@nugget/ai",
  "private": true,
  "scripts": {
    "build": "bun generate",
    "clean": "git clean -xdf .turbo node_modules dist",
    "dev": "bun generate && tsc --watch",
    "generate": "bunx @boundaryml/baml@latest generate --from ./src/baml_src",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "type": "module",
  "version": "0.1.0"
}

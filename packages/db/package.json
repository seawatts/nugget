{
  "dependencies": {
    "@clerk/nextjs": "catalog:",
    "@nugget/id": "workspace:*",
    "@nugget/logger": "workspace:*",
    "@nugget/zustand": "workspace:*",
    "@supabase/ssr": "0.7.0",
    "@supabase/supabase-js": "catalog:",
    "@t3-oss/env-core": "catalog:",
    "@vercel/postgres": "0.10.0",
    "date-fns": "catalog:",
    "drizzle-orm": "catalog:",
    "drizzle-seed": "0.3.1",
    "drizzle-zod": "catalog:",
    "lodash-es": "4.17.21",
    "next": "catalog:",
    "postgres": "3.4.7",
    "react": "catalog:",
    "react-dom": "catalog:",
    "zod": "catalog:",
    "zustand": "catalog:"
  },
  "devDependencies": {
    "@nugget/tsconfig": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "@t3-oss/env-nextjs": "catalog:",
    "@types/lodash-es": "4.17.12",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "chokidar": "4.0.3",
    "drizzle-kit": "0.31.7",
    "supabase": "2.58.5",
    "tsx": "4.20.6",
    "typescript": "catalog:",
    "zx": "8.8.5"
  },
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./schema": "./src/schema.ts",
    "./supabase/cli": "./src/supabase/cli.ts",
    "./supabase/client": "./src/supabase/client-exports.ts",
    "./supabase/create-channel": "./src/supabase/create-channel.ts",
    "./supabase/get-realtime-record": "./src/supabase/get-realtime-record.ts",
    "./supabase/rcs": "./src/supabase/server.rcs.ts",
    "./supabase/server": "./src/supabase/server-exports.ts",
    "./supabase/types": "./src/supabase/types.generated.ts"
  },
  "license": "MIT",
  "name": "@nugget/db",
  "private": true,
  "scripts": {
    "build.skip": "bun run gen-supabase-types",
    "clean": "git clean -xdf .turbo node_modules dist",
    "create-storage-policies": "tsx scripts/create-storage-policies.ts",
    "create-supabase-policies": "tsx scripts/create-supabase-policies.ts",
    "dev": "tsx ./scripts/watch.ts",
    "enable-supabase-realtime": "tsx scripts/enable-supabase-realtime.ts",
    "gen-migration": "drizzle-kit generate && bun scripts/prepend-rls-functions.ts",
    "gen-supabase-types": "tsx ./scripts/generate-supabase-types.ts",
    "migrate": "drizzle-kit migrate && bun gen-supabase-types && bun create-supabase-policies && bun create-storage-policies && bun enable-supabase-realtime",
    "push": "drizzle-kit push && bun gen-supabase-types && bun create-supabase-policies && bun create-storage-policies && bun enable-supabase-realtime",
    "seed": "tsx src/seed.ts",
    "studio": "drizzle-kit studio",
    "supabase:local": "supabase start",
    "test:clerk-supabase": "tsx scripts/test-clerk-supabase-auth.ts",
    "test:integ": "bun test --test=integ",
    "test:unit": "bun test --test=unit",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "type": "module",
  "version": "0.1.0"
}
